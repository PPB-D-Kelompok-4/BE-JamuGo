    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "firebase-admin": "^12.1.0",
    "i18next": "^23.7.16",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.7.0",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.0.3",
    "passport": "^0.7.0",
    "passport-auth0": "^1.4.4",
    "sequelize": "^6.35.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
